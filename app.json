{
  "expo": {
    "name": "MarketBriefs",
    "slug": "marketBriefs",
    "owner": "prateek2812",
    "scheme": "marketBriefs",
    "platforms": ["ios", "android", "web"],
    "runtimeVersion": "1.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/MarketBriefsLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.prateek2812.marketBriefs",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["whatsapp", "instagram", "twitter"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/MarketBriefsLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.prateek2812.marketBriefs",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{ "scheme": "marketbriefs", "host": "redirect" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "manifest": {
        "queries": [
          { "package": "com.whatsapp" },
          { "package": "com.instagram.android" },
          { "package": "com.twitter.android" }
        ]
      }
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1030825305394-hc58lnam3pc57elggvs3d9hjkpeut4ql"
        }
      ]
    ],
    "updates": {
      "url": "https://u.expo.dev/f348fc7b-94d7-4e80-ab1f-814df370353e"
    },
    "extra": {
      "facebookAppId": "743854988102436",
      "eas": {
        "projectId": "f348fc7b-94d7-4e80-ab1f-814df370353e"
      }
    }
  }
}
